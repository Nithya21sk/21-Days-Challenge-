total = 0
for i in range(1, 6):
    mark = int(input(f"Enter mark for subject {i}: "))
    total += mark
average = total / 5
if average >= 90:
    grade = "A"
elif average >= 75:
    grade = "B"
elif average >= 50:
    grade = "C"
else:
    grade = "Fail"

print("Total:", total)
print("Average:", average)
print("Grade:", grade)

ATM Withdrawal System
balance = 10000.0
attempts = 0
while attempts < 3:
    amount = float(input("Enter withdrawal amount: "))
    if amount <= balance:
        balance -= amount
        print("Withdrawal successful")
        print("Remaining balance:", balance)
        break
    else:
        print("Insufficient balance")
    
    attempts += 1

if attempts == 3:
    print("Transaction failed after 3 attempts")

Number Analysis Program
while True:
    num = int(input("Enter a number (0 to stop): "))
    
    if num == 0:
        break

    if num > 0:
        print("Positive number")
    elif num < 0:
        print("Negative number")

    if num % 2 == 0:
        print("Even number")
    else:
        print("Odd number")
